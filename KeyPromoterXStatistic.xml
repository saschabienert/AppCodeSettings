<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Back">
        <Item>
          <IdeaActionID>Back</IdeaActionID>
          <ShortCut>'Button4 Click' or '⌃⌘←'</ShortCut>
          <Description>Back</Description>
          <Count>14</Count>
        </Item>
      </Statistic>
      <Statistic Action="Forward">
        <Item>
          <IdeaActionID>Forward</IdeaActionID>
          <ShortCut>'Button5 Click' or '⌃⌘→'</ShortCut>
          <Description>Forward</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Messages">
        <Item>
          <IdeaActionID>ActivateMessagesToolWindow</IdeaActionID>
          <ShortCut>'⌘0'</ShortCut>
          <Description>Messages</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Run 'FrequencyLimitsTests...'">
        <Item>
          <IdeaActionID>RunClass</IdeaActionID>
          <ShortCut>'⌃⇧R'</ShortCut>
          <Description>Run 'FrequencyLimitsTests...'</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Stop 'Functional Tests'">
        <Item>
          <IdeaActionID>Stop</IdeaActionID>
          <ShortCut>'⌘⎋'</ShortCut>
          <Description>Stop 'Functional Tests'</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
    </option>
    <option name="suppressed">
      <Statistic Action="Declaration">
        <Item>
          <IdeaActionID>GotoDeclaration</IdeaActionID>
          <ShortCut>'F3' or '⌘Button1 Click'</ShortCut>
          <Description>Declaration</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>